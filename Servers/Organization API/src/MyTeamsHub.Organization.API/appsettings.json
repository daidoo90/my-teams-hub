{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DatabaseOptions": {
    "EnableDatabaseMigrations": true,
    "EnableSeedData": true,
    "EnableSeedTestData": false,
    "EnableSensitiveDataLogging": true,
    "EnableDetailedErrors": true
  },
  "CacheOptions": {
    "AbsoluteExpirationRelativeToNow": 30,
    "SlidingExpiration": 10
  }
}
